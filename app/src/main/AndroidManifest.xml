<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yshen.study">

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".animation.AnimationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".components.ComponentsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".components.activity.ActivityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".designmodel.DesignModelActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".framework.FrameworkActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".jetpack.JetPackActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".network.NetworkActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".optimization.OptimizationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".process.ProcessActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".storage.StorageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".thread.ThreadActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".tools.ToolsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.ViewActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>